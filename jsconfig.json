{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"strict": true,
		"target": "es2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"allowSyntheticDefaultImports": true
	},
	"include": ["./src/**/*.js", "__tests__/**/*.js"],
	"exclude": ["node_modules"]
}
